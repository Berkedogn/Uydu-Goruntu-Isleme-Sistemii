@model IEnumerable<YourNamespace.Models.Forest>
<h2>Ormanlar</h2>
<p><a asp-action="Create">Yeni</a></p>
<table class="table">
  <thead><tr><th>Ad</th><th>Şehir</th><th></th></tr></thead>
  <tbody>
  @foreach(var f in Model) {
    <tr>
      <td>@f.Name</td>
      <td>@f.City</td>
      <td>
        <a asp-action="Edit" asp-route-id="@f.Id">Düzenle</a> |
        <a asp-action="Details" asp-route-id="@f.Id">Detay</a> |
        <a asp-action="Delete" asp-route-id="@f.Id">Sil</a>
      </td>
    </tr>
  }
  </tbody>
</table>
